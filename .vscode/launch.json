{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run kernel",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/release/bin/sgemm",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Debug kernel",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/bin/sgemm",
            "cwd": "${workspaceFolder}",
            "miDebuggerPath": "/usr/local/cuda-12.4/bin/cuda-gdb", // Change this path
        }
    ]
}
